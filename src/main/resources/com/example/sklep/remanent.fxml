<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="button_remanent" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sklep.controller.RemanentController">
    <center>
        <AnchorPane prefHeight="400.0" prefWidth="400.0">
            <children>
                <TableView fx:id="remnantTableView" layoutX="50.0" layoutY="50.0" prefHeight="299.0" prefWidth="302.0">
                    <!-- Define TableColumns -->
                    <columns>
                        <TableColumn fx:id="productNameColumn" prefWidth="150.0" text="Product Name" />
                        <TableColumn fx:id="quantityColumn" prefWidth="150.0" text="Quantity" />
                    </columns>
                </TableView>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
